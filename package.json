{
  "name": "asistencias",
  "version": "0.0.1",
  "description": "Express,React Monorepo",
  "private": true,
  "main": "index.js",
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint:api": "eslint apps/api/src --fix --ext .ts",
    "lint:client": "eslint apps/client/src --fix --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:all": "npm run lint:api && npm run lint:client",
    "prepare": "husky install"
  },
  "keywords": [
    "Monorepo",
    "javascript",
    "typescript",
    "node",
    "express",
    "turborepo",
    "husky",
    "eslint",
    "precommit",
    "react",
    "vite"
  ],
  "devDependencies": {
    "prettier": "^3.2.5",
    "prisma": "^5.16.1",
    "turbo": "^2.0.6",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10.7.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "@prisma/client": "^5.16.1",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7"
  }
}
